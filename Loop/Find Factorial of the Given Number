package Loops;

import java.util.Scanner;

public class FindFactorial {
    public static void main(String[] args) {
        try (Scanner sc = new Scanner(System.in)) {
            int num ;
            int fact = 1;

            System.out.println("Enter any Positive Number");
            num = sc.nextInt();

            for (int i = 1; i<= num; i++){
                fact *= i;
            }
            System.out.println("Factorial : " + fact);
        }
    }
}
