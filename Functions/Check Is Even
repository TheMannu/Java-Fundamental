package Functions;

public class CheckIsEven {
    public static boolean isEven(int n) {
        if (n % 2 == 0){
            return true;
        } 
            return false;
    }
    public static void main(String[] args) {
        int n = 10;
        if (isEven(n)){
            System.out.println("Number Is Even");
        } else {
            System.out.println("Number is Odd");
        }
    }
}
