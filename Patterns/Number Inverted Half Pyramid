package Patterns;

import java.util.Scanner;

public class NumberInvertedHalfPyramid {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        System.out.println("Enter the value for N");
        int n = input.nextInt();
        input.close();
        for(int i=1; i<=n; i++){
            for(int j=n; j>=i; j--){
                System.out.print(i+" ");
            }
            System.out.println();
        }
    }
}
    

