package Patterns;

import java.util.*;

public class solidPattern{
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        System.out.println("Enter the number of Rows");
        int row = input.nextInt();
        System.out.println("Enter the number of columns");
        int column= input.nextInt();
        input.close();
          // outer loop for iterating over number of rows 
        for(int i=1; i<=row; i++){
             // nested inner loop for itrating over number of columns
            for(int j=1; j<=column; j++){
                // printing space, asterisk, sapce. creating pattern
                System.out.print(" * ");
            }
            System.out.println(); //move to next line after printing 
        }
    }
}

